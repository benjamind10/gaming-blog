<h1 id='test'>Display Name: {{userObj.username}}</h1>

<ul id='messages'>Chat Messages:</ul>
<form id='chat-form' onsubmit='return sendMessage();'>
  <input
    id='chat-message'
    placeholder='Enter message'
    autocomplete='off'
  />
  <input type='submit' />
</form>

